<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#FFFFFF"
    android:gravity="center_vertical"
    android:orientation="vertical"
    tools:context=".MainActivity" >
 
    
    <TableLayout 
	    
		android:layout_width="match_parent"
		
		android:layout_height="0dp"
		android:layout_weight="1"
	>
		
		<TableRow
			android:layout_weight="1"
			android:gravity="center"
			android:paddingLeft="10dp"
			android:paddingRight="10dp"
			android:paddingBottom = "20dp"
			android:paddingTop= "10dp"
		>
			<LinearLayout 
			 
			    android:orientation="vertical"
			    android:layout_width="fill_parent"
			    android:layout_height="fill_parent"
			    android:layout_weight="1"
			    android:gravity="center"
			   
		   	>

			   	<Button
			   	    android:id="@+id/botonConoce"
			   	    android:layout_width="92dp"
			   	    android:layout_height="wrap_content"
			   	    android:layout_weight="1"
			   	    android:background="@drawable/ic_conoce"
			   	    android:onClick="mostrar" 
			   	    android:tag="SITIO" />
			    
			    <TextView  
				  	android:layout_width="fill_parent" 
				  	android:layout_height="wrap_content" 
				  	android:gravity="center"
				  	android:textStyle="bold"
				  	android:textColor="#000000"
				  	android:text="@string/conoceMoraleja"
			  	/>
		    </LinearLayout>
		    
		    			<LinearLayout 
			 
			    android:orientation="vertical"
			    android:layout_width="fill_parent"
			    android:layout_height="fill_parent"
			    android:layout_weight="1"
			    android:gravity="center"
			    android:paddingRight="20dp"
		   	>

			   	<Button
			   	    android:id="@+id/botonTapas"
			   	    android:layout_width="80dp"
			   	    android:layout_height="wrap_content"
			   	    android:layout_weight="1"
			   	    android:background="@drawable/ic_decopas"
			   	    android:onClick="mostrar"
			   	    android:tag="TOMAR_ALGO" />
			    
			    <TextView  
				  	android:layout_width="fill_parent" 
				  	android:layout_height="wrap_content" 
				  	android:gravity="center"
				  	android:textStyle="bold"
				  	android:textColor="#000000"
				  	android:text="@string/deTapas"
			  	/>
		    </LinearLayout>
		    <LinearLayout 
			 
			    android:orientation="vertical"
			    android:layout_width="fill_parent"
			    android:layout_height="fill_parent"
			    android:layout_weight="1"
			    android:gravity="center"
			    android:paddingRight="20dp"
		   	>

			   	<Button
			   	    android:id="@+id/botonComer"
			   	    android:layout_width="87dp"
			   	    android:layout_height="wrap_content"
			   	    android:layout_weight="1"
			   	    android:background="@drawable/ic_comer"
			   	    android:onClick="mostrar"
			   	    android:tag="COMER_DORMIR" />
			    
			    <TextView  
				  	android:layout_width="fill_parent" 
				  	android:layout_height="wrap_content" 
				  	android:gravity="center"
				  	android:textStyle="bold"
				  	android:textColor="#000000"
				  	android:text="@string/dondeComer"
			  	/>
		    </LinearLayout>
		    
		    
 		</TableRow>
 		
 		<TableRow
			android:layout_weight="1"
			android:gravity="center"
			android:paddingLeft="10dp"
			android:paddingRight="10dp"
			android:paddingTop = "20dp"
			android:paddingBottom = "20dp"
		>
		
			
		    			<LinearLayout 
			 
			    android:orientation="vertical"
			    android:layout_width="fill_parent"
			    android:layout_height="fill_parent"
			    android:layout_weight="1"
			    android:gravity="center"
			    android:paddingRight="20dp"
		   	>

			   	<Button
			   	    android:id="@+id/botomComprar"
			   	    android:layout_width="92dp"
			   	    android:layout_height="wrap_content"
			   	    android:layout_weight="1"
			   	    android:background="@drawable/ic_decompras"
			   	    android:onClick="mostrar"
			   	    android:tag="DE_COMPRAS" />
			    
			    <TextView  
				  	android:layout_width="fill_parent" 
				  	android:layout_height="wrap_content" 
				  	android:gravity="center"
				  	android:textStyle="bold"
				  	android:textColor="#000000"
				  	android:text="@string/dondeComprar"
			  	/>
		    </LinearLayout>
		    
		   <LinearLayout 
			 
			    android:orientation="vertical"
			    android:layout_width="fill_parent"
			    android:layout_height="fill_parent"
			    android:layout_weight="1"
			    android:gravity="center"
			    android:paddingRight="20dp"
		   	>

			   	<Button
			   	    android:id="@+id/botonServicios"
			   	    android:layout_width="77dp"
			   	    android:layout_height="wrap_content"
			   	    android:layout_weight="1"
			   	    android:background="@drawable/ic_servicios"
			   	    android:onClick="mostrar"
			   	    android:tag="SERVICIOS" />
			    
			    <TextView  
				  	android:layout_width="fill_parent" 
				  	android:layout_height="wrap_content" 
				  	android:gravity="center"
				  	android:textStyle="bold"
				  	android:textColor="#000000"
				  	android:text="@string/serviciosReparaciones"
			  	/>
		    </LinearLayout>
		    
		    			<LinearLayout 
			 
			    android:orientation="vertical"
			    android:layout_width="fill_parent"
			    android:layout_height="fill_parent"
			    android:layout_weight="1"
			    android:gravity="center"
			   
		   	>

			   	<Button
			   	    android:id="@+id/botonFavoritos"
			   	    android:layout_width="wrap_content"
			   	    android:layout_height="wrap_content"
			   	    android:background="@drawable/ic_favoritos"
			   	    android:onClick="mostrarFavoritos" />
			    
			    <TextView  
				  	android:layout_width="fill_parent" 
				  	android:layout_height="wrap_content" 
				  	android:gravity="center"
				  	android:textStyle="bold"
				  	android:textColor="#000000"
				  	android:text="@string/favoritos"
			  	/>
		    </LinearLayout>
		     			
		    			
 		</TableRow>
	
 		
 		
	</TableLayout>
    
    
    <TextView  
		android:id="@+id/txBienvenidax"
    	android:layout_width="fill_parent" 
    	android:layout_height="wrap_content" 
    	android:gravity="center"
    	android:textStyle="bold"
    	android:textColor="#000000"
    	
    />
    
    <TextView  
		android:id="@+id/txBienvenida"		
    	android:layout_width="fill_parent" 
    	android:layout_height="wrap_content" 
    	android:gravity="center"
    	android:textColor="#000000"    	
    	android:textStyle="bold"
    	android:text="@string/copiright"
    />
  
</LinearLayout>
